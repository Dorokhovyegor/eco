<?xml version="1.0" encoding="utf-8"?>
<navigation android:id="@+id/navigation"
    app:startDestination="@id/profile_destination"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <fragment
        android:id="@+id/profile_destination"
        android:name="com.voodoolab.eco.ui.tab_fragments.ProfileFragment"
        android:label="ProfileFragment"
        tools:layout="@layout/profile_fragment">
        <action
            android:id="@+id/action_action_profile_to_paymentMethodFragment"
            app:destination="@+id/payment_destination"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
    </fragment>
    <fragment
        android:id="@+id/map_destination"
        android:name="com.voodoolab.eco.ui.tab_fragments.MapFragment"
        android:label="MapFragment"></fragment>
    <fragment
        android:id="@+id/discount_destination"
        android:name="com.voodoolab.eco.ui.tab_fragments.DiscountsFragment"
        android:label="DiscountsFragment"
        tools:layout="@layout/discounts_fragment" />
</navigation>